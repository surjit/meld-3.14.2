#: ../data/ui/dirdiff.ui.h:2 ../data/ui/vcview.ui.h:2
msgid "Compare selected files"
msgstr "Упоредите изабране датотеке"

#: ../data/ui/dirdiff.ui.h:3
msgid "Copy to _Left"
msgstr "Умножи _лево"

#: ../data/ui/dirdiff.ui.h:4
