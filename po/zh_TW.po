msgid "Delete"
msgstr "刪除"

#: ../data/ui/filediff.ui.h:31
msgid "Delete change"
msgstr "刪除變更"

#: ../data/ui/filediff.ui.h:32
msgid "Merge All from Left"
msgstr "從左側合併所有更改"

#: ../data/ui/filediff.ui.h:33
msgid "Merge all non-conflicting changes from the left"
